{
  "name": "csv2ofx",
  "version": "0.0.1",
  "private": false,
  "description": "Convert CSV files to OFX",
  "repository": "https://github.com/rylorin/csv2ofx.git",
  "license": "MIT",
  "author": "Ronan-Yann Lorin <ryl@free.fr>",
  "scripts": {
    "build": "tsc --project tsconfig.json",
    "clean": "rm -rf build",
    "lint": "eslint .",
    "prepublishOnly": "yarn build",
    "qc": "yarn lint && yarn type-check && prettier --check .",
    "start": "NODE_ENV=production node build/index.js",
    "start:dev": "ts-node-dev --project tsconfig.json src/index",
    "test": "yarn qc",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "config": "^4.1.0",
    "csv-parse": "^6.1.0",
    "dotenv": "^17.2.1",
    "luxon": "^3.7.1"
  },
  "devDependencies": {
    "@types/config": "^3.3.4",
    "@types/luxon": "^3.7.1",
    "@types/node": "^20.19.9",
    "concurrently": "^9.2.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "husky": "^9.1.5",
    "lint-staged": "^16.1.4",
    "prettier": "^3.6.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.38.0"
  },
  "engines": {
    "node": "^20.19.2"
  }
}
